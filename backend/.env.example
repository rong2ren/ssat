# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here 

# Supabase Configuration (MANDATORY)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# NLP Model Configuration (OPTIONAL but recommended)
EMBEDDING_MODEL_NAME=your_embedding_model_name_here

# App Configuration
APP_ENV=dev

# Frontend Supabase Configuration (NEXT_PUBLIC_ prefix required for client-side access)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
